<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
					xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
					xmlns:local="clr-namespace:Spillway.Resources"
					xmlns:vm="clr-namespace:Spillway.ViewModels"
					xmlns:v="clr-namespace:Spillway.Views">
	<DataTemplate DataType="{x:Type vm:ProfileViewModel}">
		<v:ProfileView ViewState="{Binding ViewState}" />
	</DataTemplate>

	<DataTemplate DataType="{x:Type vm:DebugViewModel}">
		<v:DebugView />
	</DataTemplate>
</ResourceDictionary>